{
  "manifest_version": 2,
  "name": "Java-Redirector",
  "version": "1.2",

  "description": "Redirects your Browser to the newest version of the javadocs",

  "icons": {
    "48": "icons/java-redirect-48.png"
  },

  "content_scripts": [
    {
      "matches": ["*://docs.oracle.com/javase/*/docs/*"],
      "js": ["java-redirect.js"]
    }
  ]
}
